# System binaries
/system/bin/pfmnceserver		u:object_r:pfmnceserver_exec:s0
/dev/graphics/galcore			u:object_r:graphics_device:s0
/dev/cpu_dma_latency			u:object_r:cpuctl_device:s0
/dev/de 						u:object_r:display_device:s0
/dev/hdmi						u:object_r:hdmi_device:s0
/dev/loop-control				u:object_r:loopctl_device:s0
/dev/rfkill						u:object_r:rfkill_device:s0
/dev/tty[0-65]*					u:object_r:tty_device:s0
/dev/ttyGS[0-5]*				u:object_r:radio_device:s0
/dev/vde						u:object_r:video_device:s0
/dev/vce						u:object_r:video_device:s0
/sys/devices/act_de/display[0-4]*?		u:object_r:sysfs_devices_actde:s0